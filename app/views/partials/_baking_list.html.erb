<h3>To Be Shipped</h3>
<table>
    <thead>
        <th>Item</th>
        <th>Unshipped Items</th>
    </thead>
    <tbody>
        <% Item.all.each do |item| %>
            <% if item.unshipped_quantity > 0 %>
                <tr>
                    <td><%= item.name %></td>
                    <td><%= item.unshipped_quantity %></td>
                </tr>
            <% end %>
        <% end %>
    </tbody>
</table>