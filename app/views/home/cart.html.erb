<% if session[:cart].nil? %>
    Cart is Nil <!-- Should never get here -->
<% elsif session[:cart].empty? %>
    Cart is Empty
<% else %>
  <%= render :partial => "partials/cart" %>
<% end %>

<%= link_to ("<i class = 'mdi-content-add left'></i> Checkout").html_safe, new_order_path, :encode => "javascript",
    :class => "btn green" %>